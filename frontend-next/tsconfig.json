{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/adapaters/*": ["adapters/*"],
      "@/components/*": ["components/*"],
      "@/config/*": ["config/*"],
      "@/utils/*": ["utils/*"],
      "@/hooks/*": ["hooks/*"],
      // "@/interceptos/*": ["interceptos/*"],
      // "@/models/*": ["models/*"],
      "@/redux/*": ["redux/*"],
      // "@/services/*": ["services/*"],
    } 
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/form/form.component.tss", "pages/dashboard/finance/components/newexpense.finance.tss"],
  "exclude": ["node_modules"],
}
